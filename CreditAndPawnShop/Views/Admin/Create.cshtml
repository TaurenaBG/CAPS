@model CAPS.ViewModels.PawnShopsViewModel

<h2>Create New PawnShop</h2>
<form method="post">
    <!-- Your form for creating a pawn shop -->
    <input type="text" name="Name" placeholder="PawnShop Name" />
    <input type="text" name="City" placeholder="City" />
    <input type="text" name="LocationUrl" placeholder="Location URL" />
    <button type="submit">Create PawnShop</button>
</form>
